{
  "name": "thecompiler",
  "version": "1.0.0",
  "description": "monorepo for thecompiler",
  "scripts": {
    "alias": "now alias",
    "build": "npm run data && next build",
    "data": "node data",
    "dev": "nodemon server/index.js",
    "precommit": "npm run data && prettier \"**/*.{js,json}\" --write && git add .",
    "serve": "node build/main",
    "start": "npm run build && npm run serve",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "downshift": "^2.1.5",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.1.1",
    "graphql-type-json": "^0.2.1",
    "grid-styled": "^5.0.2",
    "helmet": "^3.13.0",
    "lodash": "^4.17.10",
    "next": "^6.1.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-components": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-styled-components": "^1.5.1",
    "eslint": "^5.3.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.1",
    "nodemon": "^1.18.4",
    "npmlog": "^4.1.2",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.4.2",
    "react-testing-library": "^5.0.0"
  }
}
